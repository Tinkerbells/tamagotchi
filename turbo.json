{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "tasks": {
    "build": {
      "env": ["VITE_CLOUDINARY_CLOUD_NAME"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "public/build/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "env": ["VITE_CLOUDINARY_CLOUD_NAME"],
      "cache": false,
      "persistent": true
    }
  }
}
